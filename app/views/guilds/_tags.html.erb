<ul id="tag-list">
  <% @blog_tags.each do |tag| %>
    <li class="parent" onclick="func1(this)"><i class="fa fa-folder"></i>&nbsp;<%= tag.name %>&nbsp;<i class="fa fa-chevron-down fa-xs"></i>
      <ul>
        <% tag.blogs.each do |blog| %>
          <li class="child"><i class="fa fa-file-invoice"></i>&nbsp;<%= link_to blog.title, blog_path(blog), 'data-turbolinks': false %></li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>


<script>
    function func1(e) {
      e.classList.toggle("active");
    }
</script>